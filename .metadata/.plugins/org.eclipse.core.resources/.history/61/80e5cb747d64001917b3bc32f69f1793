package arrenlazado;

public class Lista <T> {
	
	private Nodo <T> lsta; 
	
	public Lista (){
		
		this.lsta = null ; 
	}
	
	public  void  insertarPrimero (T hola ) {
		
		Nodo <T> ex = new Nodo <T> (hola);
		
		if (this.lsta == null) {
			lsta.siguiente = (Nodo) ex ; 
		}else {
			ex.siguiente = lsta;
		}
		
	}
	
	public void insertarUltimo () {
		
	}
	
	
	
	
	
	
	
	
}
